<h1>Pirate Name: <%= @pirate.name %></h1>
<h1>Pirate Weight: <%= @pirate.weight %></h1>
<h1>Pirate Height: <%= @pirate.height %></h1>

<% @ships.each do |ship| %>
  <div class="ship">
    <h3>Ship Name: <%= ship.name %></h3>
    <h3>Ship Type: <%= ship.type %></h3>
    <h3>Ship Booty: <%= ship.booty %></h3>
  </div>
<% end %>
